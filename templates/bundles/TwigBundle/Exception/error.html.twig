{% extends 'errors.html.twig' %}

{% block body %}
<div class="error-container">
	<h1 class="error-title">{{ 'error.page_title'|trans({}, 'messages', app.request.locale) }}</h1>
	<p class="error-message">{{ 'error.message'|trans({}, 'messages', app.request.locale) }}</p>
	<a href="{{ path('home') }}" class="btn btn-primary">{{ 'product.back_to_home'|trans({}, 'messages', app.request.locale) }}</a>
</div>
{% endblock %}
